<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>launchpads</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.9.4/css/bulma.min.css">
</head>

<body>
    <table class="table">
        <thead>
            <tr>
                <th><abbr title="full-name">Full Name</abbr></th>

                <th><abbr title="status">status</abbr></th>

                <th><abbr title="region">region</abbr></th>

                <th><abbr title="details">details</abbr></th>
                <th><abbr title="timezone">timezone</abbr></th>
            </tr>
        </thead>
        <tbody>
            <% launchpads.forEach(function(launchpad) { %>
                <tr>
                    <td>
                        <%= launchpad.full_name %>
                    </td>

                    <td>
                        <%= launchpad.status %>
                    </td>

                    <td>
                        <%= launchpad.region %>
                    </td>

                    <td>
                        <%= launchpad.details %>
                    </td>
                    <td>
                        <%= launchpad.timezone %>
                    </td>
                </tr>
                <% }); %>
        </tbody>
    </table>

</body>

</html>